# The configuration file for bochk.
#

#
# config for the logging section
#
[logging]
log_file=recon_helper.log

# log level: debug, info, warning, error, critical
log_level=debug

# the directory to store log file, if store in current directory, leave it 
# empty. Do not put "\" at the end of the directory.
directory=
# directory=C:\Program Files\Git\git



[directory]

# where to look for input cash and positions files. Leave it empty
# if in current directory.
input=P:\Reconciliation
#input=C:\temp2
#input=Z:

# where to store converted files for recon. Leave it empty if in current 
# directory.
output=P:\Reconciliation\result
#output=C:\temp2\result
#output=Z:\result



[winscp]

# where to look for winscp.com
application=C:\Program Files (x86)\WinSCP\WinSCP.com

# where to store the script files
script_dir=C:\Program Files\Git\git\reconciliation_helper\winscp_script

# where to store the winscp log files
log_dir=C:\Program Files\Git\git\reconciliation_helper\winscp_log

# the time limit for a sftp job in seconds
timeout=180



[test]

# For unittest only, default value is empty (not in test mode). When doing
# unittest, the test code will set it to a different value.
testmode=